{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": "off",
    "comma-dangle": ["error", "never"],
    "max-len": ["error", { "code": 90 }],
    "eol-last": ["error", "never"],
    "arrow-parens": "off",
    "require-jsdoc": "off",
    "linebreak-style": ["error", "windows"],
    "object-curly-spacing": "off",
    "no-debugger":"off",
    "indent": ["warn", 2, { "ignoreComments": true }]
  },
  "extends": [ "eslint:recommended", "google" ]
}