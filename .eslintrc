{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": "off",
    "comma-dangle": ["error", "never"],
    "max-len": ["error", { "code": 85 }],
    "eol-last": ["error", "never"],
    "arrow-parens": "off",
    "require-jsdoc": "off",
    "linebreak-style": ["error", "windows"]
  },
  "extends": [ "eslint:recommended", "google" ]
}